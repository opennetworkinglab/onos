                  "type": "object",
                  "title": "instruction",
                  "required": [
                    "type",
                    "port"
                  ],
                  "properties": {
                    "type": {
                      "type": "string",
                      "example": "OUTPUT"
                    },
                    "port": {
                      "type": "string",
                      "example": "CONTROLLER"
                    }
                  }
                }
              },
